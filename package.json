{
  "name": "unplugin-dts-mono",
  "version": "4.5.3",
  "type": "module",
  "packageManager": "pnpm@9.15.3",
  "private": true,
  "author": "qmhc",
  "scripts": {
    "build": "tsx scripts/build.ts",
    "changelog": "pnpm -r run changelog",
    "dev": "pnpm -C packages/unplugin-dts dev",
    "demo:react": "pnpm -C examples/react build",
    "demo:svelte": "pnpm -C examples/svelte build",
    "demo:ts": "pnpm -C examples/ts build",
    "demo:ts-esbuild": "pnpm -C examples/ts-esbuild build",
    "demo:ts-rolldown": "pnpm -C examples/ts-rolldown build",
    "demo:ts-rspack": "pnpm -C examples/ts-rspack build",
    "demo:ts-webpack": "pnpm -C examples/ts-webpack build",
    "demo:vue": "pnpm -C examples/vue build",
    "demo:vue-rolldown": "pnpm -C examples/vue-rolldown build",
    "demo:vue-rspack": "pnpm -C examples/vue-rspack build",
    "lint": "eslint --cache \"**/*.{js,cjs,mjs,ts,tsx,vue}\"",
    "precommit": "lint-staged -c ./.husky/.lintstagedrc -q --allow-empty",
    "prepare": "husky init",
    "prepublishOnly": "pinst --disable",
    "prettier": "pretty-quick --staged && pnpm run lint",
    "postpublish": "pinst --enable",
    "publish:ci": "tsx scripts/publish.ts",
    "release": "tsx scripts/release.ts",
    "test": "pnpm -r run test"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@rspack/core": "^1.3.9",
    "@types/debug": "^4.1.12",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.15.3",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.7.0",
    "@vexip-ui/commitlint-config": "^0.5.0",
    "@vexip-ui/eslint-config": "^0.12.1",
    "@vexip-ui/prettier-config": "^1.0.0",
    "@vexip-ui/scripts": "^1.2.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^8.57.0",
    "execa": "^9.5.2",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^15.5.1",
    "minimist": "^1.2.8",
    "pinst": "^3.0.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "rolldown": "1.0.0-beta.8",
    "semver": "^7.7.1",
    "tsx": "^4.19.3",
    "typescript": "5.8.3",
    "unbuild": "^3.5.0",
    "vite": "^6.3.3",
    "vitest": "^3.1.2",
    "webpack": "^5.99.7"
  }
}
